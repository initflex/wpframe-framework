{
    "name": "initflex/wpframe-framework",
    "description": "WPFrame Framework.",
    "keywords": ["wordpress", "framework wordpress", "wordpress plugin", "plugin framework", "wordpress plugin framework", "WPFrame Framework"],
    "type": "library",
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {},
    "license": "MIT",
    "authors": [{
        "name": "initflex",
        "email": "initflex22@gmail.com"
    }],
    "support": {
        "issues": "https://github.com/initflex/wpframe-framework/issues",
        "source": "https://github.com/initflex/wpframe-framework"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {}
    },
    "autoload": {
        "files": [
            "src/Support/functions.php"
        ], 
        "psr-4": {
            "Wpframe\\Sys\\Container\\": "src/Container",
            "Wpframe\\Sys\\Database\\": "src/Database",
            "Wpframe\\Sys\\Exceptions\\": "src/Exceptions",
            "Wpframe\\Sys\\Http\\": "src/Http",
            "Wpframe\\Sys\\Logger\\": "src/Logger",
            "Wpframe\\Sys\\Routing\\": "src/Routing",
            "Wpframe\\Sys\\Support\\": "src/Support",
            "Wpframe\\Sys\\Traits\\": "src/Traits"
        }
    },
    "autoload-dev": {}
}
